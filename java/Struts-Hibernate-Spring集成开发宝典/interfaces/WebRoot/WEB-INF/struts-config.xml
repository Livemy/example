<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
  <data-sources />
  <form-beans />
  <global-exceptions />
  <global-forwards />
  <action-mappings >
    <action path="/barChart" type="cn.hxex.interfaces.struts.action.BarChartAction">
      <forward name="display" path="/barchart.jsp" />
    </action>
    <action path="/dynaStyle" type="cn.hxex.interfaces.struts.action.DynaStyleAction">
      <forward name="display" path="/dynastyle.jsp" />
    </action>
    <action path="/page" type="cn.hxex.interfaces.struts.action.PageAction">
      <forward name="display" path="/page.jsp" />
    </action>
    
    
    
    

  </action-mappings>

  <message-resources parameter="cn.hxex.interfaces.struts.ApplicationResources" />
  
  
</struts-config>

